.uploadImg-container{
    box-sizing: border-box;
    width: 100%;
}

.uploadImg-title{
    box-sizing: border-box;
    height: 115rpx;
    padding: 0 0 0 32rpx;
    line-height: 115rpx;
    width: 100%;
    color: #333333;
    font-size: 36rpx;
    margin: 0;
}

.uploadImg-add{
    box-sizing: border-box;
    width: 150rpx;
    height: 150rpx;
    margin: 30rpx;
    border-radius: 12rpx;
    background-color: #f5f5f5;
    border: 1px solid #e5e5e5;
    display: flex;
    display: -webkit-flex;
    justify-content: center;
    align-items: center;
}

.uploadImg-add .jia-img{
    box-sizing: border-box;
    width: 75rpx;
    height: 75rpx;
    background-color: transparent;
    outline: none;
    margin: 0;
}



.scroll-box{
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    position: fixed;
}

.scroll-ul-box{
    box-sizing: border-box;
    position: relative;
    width: 48%;
    padding: 0 1%;
    margin: 0 1%;
    display: inline-block;
    top: 0;
    vertical-align: top;
}

.sroll-image{
    box-sizing: border-box;
    width: 100%;
    /* min-height: 200rpx; */
    background-size: cover;
    background-repeat: no-repeat;
}